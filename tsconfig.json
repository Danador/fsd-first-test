{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@shared/*": [
				"src/shared/*"
			],
			"@entities*": [
				"src/entities*"
			],
			"@features/*": [
				"src/features/*"
			],
			"@widgets/*": [
				"src/widgets/*"
			],
			"@images/*": [
				"public/images/*"
			],
			"@data/*": [
				"src/data/*"
			],
			"@pages/*": [
				"src/pages/*"
			],
			"@processes/*": [
				"src/processes/*"
			],
			"@app/*": [
				"src/app/*"
			]
		},
		"jsx": "preserve"
	}
}
